<Home>:
	name:"home"
	Image:
		source:"1X/bg.png"
	AnchorLayout:
		anchor_y:"top"
		Button:
			background_color:(1,1,1,1)
			disabled:True
			size_hint:(100,.1)
			background_normal:""   
	Button:
		text:"Help"
		size_hint:(.3,.08)
		pos_hint:{"x":.69,"y":.91}
		color:"black"
		on_press:
			root.manager.transition.direction="left"
			app.root.current="help"
	Label:
		text:"Quiz"
		size_hint:(.3,.08)
		pos_hint:{"x":.0,"y":.91}
		color:"yellow"
	Button:
		text:"Exit"
		size_hint:(.3,.08)
		pos_hint:{"x":.35,"y":.91}
		color:"black"''
		on_press:animate(self)
	Image:
		source:"bg.png"
		allow_stretch:True
		pos_hint:{"x":.1,"y":.2}
		size_hint:(.8,.8)
	RoundedButton:
		text:"Start Quiz"
		size_hint:(.5,.1)
		color:"black"
		on_press:print("Started")
		pos_hint:{"x":.25,"y":.15}
		on_press:
		    app.root.current='q1'
		    root.manager.transition.direction="left"
<RoundedButton@Button>:
	background_color:(0,0,0,0)
	background_normal:""
	canvas.before:
		Color:
			rgba:(1,1,0,1)
		RoundedRectangle:
			size:self.size
			pos:self.pos
			radius:[30]

